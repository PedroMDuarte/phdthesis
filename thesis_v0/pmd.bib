@article{Kanamori01091963,
author = {Kanamori, Junjiro}, 
title = {Electron Correlation and Ferromagnetism of Transition Metals},
volume = {30}, 
number = {3}, 
pages = {275-289}, 
year = {1963}, 
doi = {10.1143/PTP.30.275}, 
abstract ={The electron correlation in a narrow energy band is discussed taking into account the multiple scattering between two electrons. The discussion is an adaptation of Brueckner's theory of nuclear matter. It is assumed that electrons interact with each other only when they are at the same atom. The effect of the electron correlation depends in an intricate way on the energy spectrum of a given band. An approximate expression of the effective magnitude of the interaction is derived. The condition for the occurrence of ferromagnetism is investigated for various types of bands. The ferromagnetism of Ni and the paramagnetism of Pd can be understood reasonably through the present approach. The degeneracy of the d bands is taken into account in the discussion of these metals.}, 
URL = {http://ptp.oxfordjournals.org/content/30/3/275.abstract}, 
eprint = {http://ptp.oxfordjournals.org/content/30/3/275.full.pdf+html}, 
journal = {Progress of Theoretical Physics} 
}

